class Solution:
    def maximum69Number(self, num: int) -> int:
        placeValue = 1
        placeValue6 = -1
        temp = num
        while temp > 0:
            remain = temp % 10
            if remain == 6:
                placeValue6 = placeValue
            temp //= 10
            placeValue *= 10
        if placeValue6 == -1:
            return num
        return num + 3 * placeValue6
