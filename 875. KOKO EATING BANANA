class Solution:
    def minEatingSpeed(self, piles: List[int], h: int) -> int:
        high,low=1,max(piles)
        ans=low
        while high<=low:
            mid=(high+low)//2
            hours=sum((p+mid-1)//mid for p in piles)
            if hours<=h:
                ans=mid
                low=mid-1
            else:
                high=mid+1
        return ans

        
